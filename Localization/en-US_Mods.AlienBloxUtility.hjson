Documents: {
	Test: {
		Title: Test Document
		Description: little pig
	}

	Lua: {
		Title: Lua
		Description:
			'''
			Lua in AlienBlox's Utility is a scripting language used
			to modify the behavior of tModLoader, it is useful for debugging.
			AlienBlox's Utility uses NeoLua for Lua
			'''
	}

	AlieBloxUtilityDoc: {
		Title: AlienBlox's Utility
		Description:
			'''
			AlienBlox's Utility is a tModLoader mod used for debugging
			It has many features like a decompiler, interactive console
			and JavaScript + Lua scripting support!
			'''
	}

	Console: {
		Title: Interactive Console
		Description:
			'''
			The Interactive Console is an utility in AlienBlox's Utility
			Used for running commands, Lua and JavaScript code, things also
			print to the interactive console like Lua or JavaScript output.
			'''
	}

	Decompiler: {
		Title: Decompiler
		Description:
			'''
			The Decompiler in AlienBlox's Utility is used
			for decompiling mods and extracting assets, it
			is async'd so it is thread safe and won't crash tModLoader
			from heavy computation on its main thread
			'''
	}

	StatsBar: {
		Title: Stats Bar
		Description:
			'''
			The Stats Bar in AlienBlox's Utility is
			used for seeing vanilla stats, it is useful
			for tracking damage modifiers
			'''
	}

	JavaScript: {
		Title: JavaScript
		Description:
			'''
			JavaScript in AlienBlox's Utility is a scripting language used
			to modify the behavior of tModLoader, it is useful for debugging.
			AlienBlox's Utility uses Jint + Acornima for JavaScript
			'''
	}

	ButcherCommand: {
		Title: Butcher Command
		Description:
			'''
			The Butcher Command in AlienBlox's Utility is a command used for one shotting things
			It has these different options: item, npc, npcfriendly and proj
			'''
	}

	TModLoaderRunCommand: {
		Title: tModLoader Command
		Description: Runs an extra instance of tModLoader
	}

	DMCommand: {
		Title: DM Command
		Description: Allows the direct messaging of users.
	}
}

